{
  "expo": {
    "name": "Notes App",
    "slug": "notes-app",
    "version": "1.0.0",
    "owner": "mehmetkekec",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-with-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mehmetkekec.notesapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
